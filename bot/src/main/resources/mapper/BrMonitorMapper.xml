<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dahuaboke.mpda.bot.monitor.mapper.BrMonitorMapper">

    <select id="streamMonitorEvent" resultType="com.dahuaboke.mpda.bot.monitor.entity.MonitorEventEntity">
        select *
        from br_monitor
        where
            DATE (TO_TIMESTAMP(createTime
            , 'YYYY-MM-DD HH24:MI:SS.MS'))
            BETWEEN
            DATE (#{begin})
          AND
            DATE (#{end})
    </select>

</mapper>